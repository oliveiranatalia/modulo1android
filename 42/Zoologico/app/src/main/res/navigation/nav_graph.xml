<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/cadastroFragment">

    <fragment
        android:id="@+id/cadastroFragment"
        android:name="br.com.zup.zoologico.CadastroFragment"
        android:label="Zoologico"
        tools:layout="@layout/fragment_cadastro" >
        <action
            android:id="@+id/action_cadastroFragment_to_descricaoFragment"
            app:destination="@id/descricaoFragment" />
    </fragment>
    <fragment
        android:id="@+id/descricaoFragment"
        android:name="br.com.zup.zoologico.DescricaoFragment"
        android:label="Zoologico"
        tools:layout="@layout/fragment_descricao" >
        <action
            android:id="@+id/action_descricaoFragment_to_cadastroFragment"
            app:destination="@id/cadastroFragment" />
    </fragment>
</navigation>